{
  "ai_endpoint": "https://your-ai-server.com/api/v1/chat/completions",
  "token": "your-api-token-here",
  "model": "gpt-4",
  "custom_prompt": "请特别注意以下情况：\n1. 数据库连接问题\n2. 内存泄漏警告\n3. 安全相关日志\n4. 性能瓶颈指标\n\n请根据这些特殊要求调整判断标准。",
  "max_retries": 3,
  "timeout": 30,
  "rate_limit": 100,
  "local_filter": true,
  "ai_services": [
    {
      "name": "openai",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "token": "sk-your-openai-token",
      "model": "gpt-4",
      "priority": 1,
      "enabled": true
    },
    {
      "name": "azure",
      "endpoint": "https://your-resource.openai.azure.com/openai/deployments/gpt-4/chat/completions?api-version=2024-02-15-preview",
      "token": "your-azure-key",
      "model": "gpt-4",
      "priority": 2,
      "enabled": true
    },
    {
      "name": "anthropic",
      "endpoint": "https://api.anthropic.com/v1/messages",
      "token": "sk-ant-your-anthropic-token",
      "model": "claude-3-sonnet-20240229",
      "priority": 3,
      "enabled": false
    }
  ],
  "default_ai": "openai",
  "rules": [
    {
      "id": "debug_logs",
      "name": "调试日志过滤",
      "pattern": ".*\\[DEBUG\\].*",
      "action": "ignore",
      "priority": 1,
      "description": "过滤掉所有DEBUG级别的日志",
      "enabled": true,
      "category": "log_level",
      "color": ""
    },
    {
      "id": "error_alerts",
      "name": "错误告警",
      "pattern": ".*\\[ERROR\\].*|.*Exception.*|.*Error.*",
      "action": "alert",
      "priority": 2,
      "description": "标记错误日志为告警",
      "enabled": true,
      "category": "error",
      "color": "red"
    },
    {
      "id": "database_errors",
      "name": "数据库错误高亮",
      "pattern": ".*(database|db|mysql|postgres|mongodb).*error.*",
      "action": "highlight",
      "priority": 3,
      "description": "高亮显示数据库相关错误",
      "enabled": true,
      "category": "database",
      "color": "yellow"
    },
    {
      "id": "security_events",
      "name": "安全事件告警",
      "pattern": ".*(security|auth|login|permission|access).*",
      "action": "alert",
      "priority": 4,
      "description": "标记安全相关事件为告警",
      "enabled": true,
      "category": "security",
      "color": "magenta"
    },
    {
      "id": "performance_warnings",
      "name": "性能警告",
      "pattern": ".*(slow|timeout|latency|performance).*",
      "action": "highlight",
      "priority": 5,
      "description": "高亮显示性能相关警告",
      "enabled": true,
      "category": "performance",
      "color": "orange"
    }
  ],
  "cache": {
    "max_size": 104857600,
    "max_items": 1000,
    "default_ttl": "1h",
    "ai_ttl": "24h",
    "rule_ttl": "1h",
    "config_ttl": "30m",
    "cleanup_interval": "5m",
    "enabled": true
  },
  "notifiers": {
    "email": {
      "enabled": false,
      "provider": "smtp",
      "host": "smtp.gmail.com",
      "port": 587,
      "username": "your-email@gmail.com",
      "password": "your-app-password",
      "from_email": "your-email@gmail.com",
      "to_emails": ["admin@company.com", "devops@company.com"]
    },
    "_email_resend_example": {
      "enabled": false,
      "provider": "resend",
      "host": "",
      "port": 0,
      "username": "",
      "password": "re_xxxxxxxxxxxxx",
      "from_email": "alerts@yourdomain.com",
      "to_emails": ["admin@company.com"]
    },
    "dingtalk": {
      "enabled": false,
      "url": "https://oapi.dingtalk.com/robot/send?access_token=YOUR_TOKEN"
    },
    "wechat": {
      "enabled": false,
      "url": "https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=YOUR_KEY"
    },
    "feishu": {
      "enabled": false,
      "url": "https://open.feishu.cn/open-apis/bot/v2/hook/YOUR_TOKEN"
    },
    "slack": {
      "enabled": false,
      "url": "https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK"
    },
    "custom_webhooks": [
      {
        "enabled": false,
        "url": "https://your-custom-webhook.com/endpoint",
        "secret": "your-webhook-secret"
      }
    ]
  }
}
